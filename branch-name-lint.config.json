{
  "branchNameLinter": {
    "prefixes": [
      "build",
      "ci",
      "chore",
      "docs",
      "feat",
      "fix",
      "perf",
      "refactor",
      "revert",
      "style",
      "test"
    ],
    "suggestions": {
      "cicd": "ci",
      "shore": "chore",
      "doc": "docs",
      "features": "feat",
      "feature": "feat",
      "fixes": "fix",
      "hotfix": "fix",
      "performance": "perf",
      "performances": "perf",
      "refactors": "refactor",
      "ref": "refactor",
      "rev": "revert",
      "styles": "style",
      "tests": "test"
    },
    "disallowed": [
      "master"
    ],
    "seperator": "/",
    "msgBranchDisallowed": "Commit to \"%s\" is not allowed, create other branch and make a PR for this feature",
    "msgPrefixNotAllowed": "Branch prefix \"%s\" is not allowed.",
    "msgPrefixSuggestion": "\"%s\" should be replaced to \"%s\".",
    "msgSeperatorRequired": "Branch \"%s\" must contain a seperator \"%s\"\nExample:  feat/foo, ci/bar, fix/baz."
  }
}
